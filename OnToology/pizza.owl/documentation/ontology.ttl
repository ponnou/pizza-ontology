#################################################################
#    Object Properties
#################################################################

###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasBase
<http://www.co-ode.org/ontologies/pizza/pizza.owl#hasBase> rdf:type owl:ObjectProperty ;
                                                           rdfs:subPropertyOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#hasIngredient> ;
                                                           owl:inverseOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#isBaseOf> ;
                                                           rdf:type owl:FunctionalProperty ,
                                                                    owl:InverseFunctionalProperty ;
                                                           rdfs:domain <http://www.co-ode.org/ontologies/pizza/pizza.owl#Pizza> ;
                                                           rdfs:range <http://www.co-ode.org/ontologies/pizza/pizza.owl#PizzaBase> .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasCountryOfOrigin
<http://www.co-ode.org/ontologies/pizza/pizza.owl#hasCountryOfOrigin> rdf:type owl:ObjectProperty .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasIngredient
<http://www.co-ode.org/ontologies/pizza/pizza.owl#hasIngredient> rdf:type owl:ObjectProperty ;
                                                                 owl:inverseOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#isIngredientOf> ;
                                                                 rdf:type owl:TransitiveProperty ;
                                                                 rdfs:domain <http://www.co-ode.org/ontologies/pizza/pizza.owl#Food> ;
                                                                 rdfs:range <http://www.co-ode.org/ontologies/pizza/pizza.owl#Food> ;
                                                                 rdfs:comment "NB Transitive - the ingredients of ingredients are ingredients of the whole"@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasSpiciness
<http://www.co-ode.org/ontologies/pizza/pizza.owl#hasSpiciness> rdf:type owl:ObjectProperty ,
                                                                         owl:FunctionalProperty ;
                                                                rdfs:range <http://www.co-ode.org/ontologies/pizza/pizza.owl#Spiciness> ;
                                                                rdfs:comment "A property created to be used with the ValuePartition - Spiciness."@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#hasTopping
<http://www.co-ode.org/ontologies/pizza/pizza.owl#hasTopping> rdf:type owl:ObjectProperty ;
                                                              rdfs:subPropertyOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#hasIngredient> ;
                                                              owl:inverseOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#isToppingOf> ;
                                                              rdf:type owl:InverseFunctionalProperty ;
                                                              rdfs:domain <http://www.co-ode.org/ontologies/pizza/pizza.owl#Pizza> ;
                                                              rdfs:range <http://www.co-ode.org/ontologies/pizza/pizza.owl#PizzaTopping> ;
                                                              rdfs:comment "Note that hasTopping is inverse functional because isToppingOf is functional"@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#isBaseOf
<http://www.co-ode.org/ontologies/pizza/pizza.owl#isBaseOf> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#isIngredientOf> ;
                                                            rdf:type owl:FunctionalProperty ,
                                                                     owl:InverseFunctionalProperty .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#isIngredientOf
<http://www.co-ode.org/ontologies/pizza/pizza.owl#isIngredientOf> rdf:type owl:ObjectProperty ,
                                                                           owl:TransitiveProperty ;
                                                                  rdfs:comment "The inverse property tree to hasIngredient - all subproperties and attributes of the properties should reflect those under hasIngredient."@en .


###  http://www.co-ode.org/ontologies/pizza/pizza.owl#isToppingOf
<http://www.co-ode.org/ontologies/pizza/pizza.owl#isToppingOf> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf <http://www.co-ode.org/ontologies/pizza/pizza.owl#isIngredientOf> ;
                                                               rdf:type owl:FunctionalProperty ;
                                                               rdfs:comment "Any given instance of topping should only be added to a single pizza (no cheap half-measures on our pizzas)"@en .


